 DATE_FORMAT(a.DAT_CREATIONDATE,'%d-%b-%Y')
FROM inv_t_asset a,com_m_staff b,inv_t_vendorgoodsreceived c
WHERE a.CHR_EMPID=b.CHR_EMPID
