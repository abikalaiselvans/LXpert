 com_m_company g, com_m_desig i,com_m_bloodgroup j, com_m_employeecategory q   WHERE a.INT_DEPARTID = c.INT_DEPARTID
 AND a.INT_OFFICEID= b.INT_OFFICEID    AND a.CHR_EMPID =d.CHR_EMPID    AND a.CHR_QUALIFICATION =e.INT_QUALIFICATIONID
 AND a.INT_BRANCHID =f.INT_BRANCHID    AND a.INT_COMPANYID =g.INT_COMPANYID
 AND a.INT_DESIGID= i.INT_DESIGID    AND a.CHR_BG=j.INT_BLOODGROUPID    AND a.CHR_TYPE !='T'
 AND a.CHR_CATEGORY= q.INT_EMPLOYEECATEGORYID   AND  a.CHR_TYPE !='T'  AND  a.INT_COMPANYID= 1
