	<queryString>
		<![CDATA[SELECT (@c:=@c+1),a.CHR_APPCODE,a.CHR_APPNAME,a.CHR_APPFATHER,DATE_FORMAT(a.DT_APPLICATIONDATE,'%d-%m-%Y'),
  a.CHR_JOBGRADE,f.CHR_JOBCODE,f.CHR_JOBTITLE,a.CHR_GENDER,DATE_FORMAT(a.DT_DOB,'%d-%m-%Y'),g.CHR_GROUPNAME,
  a.CHR_MARITALSTATUS,DATE_FORMAT(a.DT_WEDDATE,'%d-%m-%Y'), h.CHR_QUALIFICATIONNAME, CHR_EMAILID,CHR_MOBILE,
  CHR_PASSPORT,CHR_PAN,CHR_BIKESTATUS,CHR_DRIVINGLIC,
  CONCAT(CHR_PADD1,' ,',CHR_PADD2,' ,',e.CHR_CITYNAME,' ,',d.CHR_DISTRICT,' ,',c.CHR_STATENAME,' ,',b.CHR_COUNTRYNAME,' , PIN :',INT_PPIN,' ,',INT_PPHONE) add1,
  CONCAT(CHR_CADD1,' ,',CHR_CADD2,' ,',e.CHR_CITYNAME,' ,',d.CHR_DISTRICT,' ,',c.CHR_STATENAME,' ,',b.CHR_COUNTRYNAME,' , PIN :',INT_CPIN,' ,',INT_CPHONE) add2
  FROM hrm_m_application a ,com_m_country b,com_m_state c,com_m_district d,com_m_city e,hrm_m_jobdesign f,com_m_bloodgroup g,com_m_qualification h ,  (SELECT @c:=0) t
  WHERE a.INT_PCOUNTRYID =b.INT_COUNTRYID AND a.INT_PSTATE=c.INT_STATEID
  AND a.INT_PDISTRICTID=d.INT_DISTRICTID AND a.INT_PCITY=e.INT_CITYID
  AND a.INT_CCOUNTRY =b.INT_COUNTRYID AND a.INT_CSTATE=c.INT_STATEID
  AND a.INT_CDISTRICTID=d.INT_DISTRICTID AND a.INT_CCITY=e.INT_CITYID
  AND a.INT_JOBID =f.INT_JOBID AND a.CHR_BG =g.INT_BLOODGROUPID
   AND a.CHR_QUALIFICATION = h.INT_QUALIFICATIONID
   AND a.CHR_MONTH ='January' AND a.INT_YEAR = 2015
    ORDER BY a.CHR_APPNAME]]>
	</queryString>
